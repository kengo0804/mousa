<div class="schedule-topbox">
</div>

<div class="schedule-text">
	<h1>Live schedule</h1>

  <%= form_for(@live_schedules) do |f| %>

	 <h4>Image</h4>
	 <%= f.attachment_field :image %>

	 <h4>live_title</h4>
	 <%= f.text_field :live_title %>

	 <h4>Live Price</h4>
	 <%= f.text_field :live_price %>

	 <div class="add-box"><br>
		<%= f.submit 'Add',class: 'btn btn-primary btn-block' %>
	 </div>
  <% end %>

</div>

